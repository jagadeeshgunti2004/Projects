   "source": [
    "n = int(input(\"Enter a positive integer: \"))\n",
    "sum_of_evens = 0\n",
    "for num in range(1, n + 1):\n",
    "    if num % 2 == 0:  \n",
    "        sum_of_evens += num\n",
    "print(f\"The sum of all even numbers between 1 and {n} is: {sum_of_evens}\")"
